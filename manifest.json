{
	"name": "wTime",
	"short_name": "wTime",
	"manifest_version": 2,
	"version": "0.1.0",
	"description": "Waka Time Projects Monitor",
	"browser_action": {
		"default_icon": "48.png",
		"default_popup": "projects.html"
	},
	"author": "rafa.cesar@gmail.com",
	"incognito": "split",
	"background": {
		"scripts": ["general.js", "events.js"],
		"persistent": false
	},
	"permissions": [
		"https://wakatime.com/",
		"notifications",
		"alarms"
	]
}